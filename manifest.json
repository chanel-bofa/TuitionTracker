{
	"manifest_version": 2,
	"name": "Tuition Tracker",
	"version": "1.0",
	"description": "This extension calculates how much money you waste when using chrome during classtime",
	"icons": {
		"128": "icon128.png",
		"64": "icon64.png", 
		"32": "icon32.png", 
		"16": "icon16.png"
	},
	"browser_action": {
		"default_icon": "icon16.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage"
	]
